<div class="panel panel-primary">
  <div class="panel-heading">Filter Projects</div>
  <div class="panel-body">
    <form>
      <div class="checkbox">
        <label>
          <input type="checkbox" data-status="pending" checked="checked"> Pending
        </label>
      </div>
      <div class="checkbox">
        <label>
          <input type="checkbox" data-status="open" checked="checked"> Open
        </label>
      </div>
      <div class="checkbox">
        <label>
          <input type="checkbox" data-status="completed" checked="checked"> Completed
        </label>
      </div>
      <div class="checkbox">
        <label>
          <input type="checkbox" data-status="confirmed" checked="checked"> Confirmed
        </label>
      </div>
    </form>
  </div>
</div>
